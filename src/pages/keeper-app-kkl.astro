            ),
            h('select', {
              value: selectedTeam,
              onChange: (e) => setSelectedTeam(e.target.value)
            },
              h('option', { value: 'all' }, 'All Teams'),
              teams.map(team => h('option', { key: team.name, value: team.name }, team.name))
            )
          )
        ),
        
        // Teams
        h('div', { className: 'teams-container' },
          h('div', { className: 'teams-list' },
            filteredTeams.map(team =>
              h('div', { key: team.name, className: 'team-card' },
                h('div', { className: 'team-header' },
                  h('div', { className: 'team-name' }, team.name),
                  h('div', { className: 'team-owner' }, team.owner)
                ),
                h('div', { className: 'table-wrapper' },
                  h('table', null,
                    h('thead', null,
                      h('tr', null,
                        h('th', null, 'Player'),
                        h('th', null, 'Current Acq.'),
                        h('th', null, 'Current Years'),
                        h('th', null, 'Status'),
                        h('th', null, '2026 Round'),
                        h('th', null, 'Years Left')
                      )
                    ),
                    h('tbody', null,
                      team.players.map((player, idx) =>
                        h('tr', {
                          key: idx,
                          className: player.keeperStatus.eligible ? 'eligible' : 'ineligible'
                        },
                          h('td', { className: 'player-name' }, player.Player),
                          h('td', { className: 'player-data' }, player.Acquired || '—'),
                          h('td', { className: 'player-data' }, player.Years || '—'),
                          h('td', null,
                            player.keeperStatus.eligible
                              ? h('div', { className: 'status-eligible' },
                                  h(CheckIcon),
                                  h('span', null, 'Eligible')
                                )
                              : h('div', { className: 'status-ineligible' },
                                  h(XIcon),
                                  h('span', null, player.keeperStatus.reason)
                                )
                          ),
                          h('td', null,
                            player.keeperStatus.eligible
                              ? h('div', { className: 'round-badge' },
                                  h('span', { className: 'round-number' }, player.keeperStatus.nextRound),
                                  player.keeperStatus.isRookie && h('span', { className: 'rookie-tag' }, '(R)')
                                )
                              : h('span', { className: 'placeholder' }, '—')
                          ),
                          h('td', null,
                            player.keeperStatus.eligible
                              ? h('span', { className: 'years-remaining' }, 
                                  `${player.keeperStatus.yearsRemaining} ${player.keeperStatus.yearsRemaining === 1 ? 'year' : 'years'}`
                                )
                              : h('span', { className: 'placeholder' }, '—')
                          )
                        )
                      )
                    )
                  )
                )
              )
            )
          )
        ),
        
        // Rules
        h('div', { className: 'rules-container' },
          h('div', { className: 'rules-card' },
